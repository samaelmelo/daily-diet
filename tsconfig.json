{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@assets/*": ["./src/assets/*"],
			"@utils/*": ["./src/utils/*"],
			"@storage/*": ["./src/storage/*"],
			"@src/*": ["./src/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
